/**app.wxss**/
page {
  height: 100%;
  color: #333;
  background-color: #f4f4f4;
  font-family: PingFangSC-Light,helvetica,Heiti SC;
}

/*轮播图盒子*/
.swiper_content{
  height: 240rpx;
  background-color: #00b8b8;
  margin-bottom: 86rpx;/*因轮播图下沉  故设外下边距;86rpx刚好到达图片下边*/
}
/*轮播图*/
.swiper_content swiper {
  width: 95%; /*宽度占比*/ 
  height: 324rpx;
  margin: 0 auto;/* 水平居中*/
}
.swiper_content swiper image {
  width: 100%;/*与父元素宽度一致*/
  border-radius: 20rpx;/* 边框圆角*/
}

/*服务图标栏 */
/* 思路是根据for循环的子项多少实现高度自动 故我没设高度，也就设不了内边距，故在子项图标中设了下边距*/
.serve{
  display: flex;/* 水平分布 */
  flex-wrap: wrap;/*可换行*/
  background-color: #FFFFFF;
  width: 95%;/*容器宽度*/
  margin: 20rpx auto;/*居中 上下分割；外边距可以重叠*/
  border-radius: 16rpx;/* 边框圆角*/
}
/*图标栏 标题*/
.serve .sTitle{
  width: 100%;/*独占一行*/
  font-size: 30rpx;
}
/*子项图标*/
.serve .sItem{
  display: flex;
  flex-direction: column;/*上下布局*/
  align-items: center;/* 水平居中 */
  width: 20%;
  height: 120rpx;
  font-size: 24rpx;
  margin-bottom: 14rpx;
}


/*样式  网络字体*/
@font-face {
  font-family:iconfont;
  src:url(data:application/font-woff;charset=utf-8;base64,) format("woff");
}

